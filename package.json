{
    "name": "purs-hotkeys",
    "displayName": "Purescript hotkeys",
    "description": "Hotkeys for getting expression types",
    "publisher": "br4ch1st0chr0n3",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/br4ch1st0chr0n3/vscode-favorite-keybindings"
    },
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Keymaps"
    ],
    "contributes": {
        "keybindings": [
            {
                "key": "ctrl+alt+z",
                "when": "editorHasSelection",
                "command": "extension.multiCommand.execute",
                "args": {
                    "command": "multiCommand.addTypeHole"
                }
            },
            {
                "key": "ctrl+alt+z",
                "when": "editorTextFocus && !editorHasSelection",
                "command": "extension.multiCommand.execute",
                "args": {
                    "command": "multiCommand.addTypeHoleAutoSelect"
                }
            },
            {
                "key": "ctrl+alt+x",
                "when": "editorTextFocus",
                "command": "extension.multiCommand.execute",
                "args": {
                    "command": "multiCommand.removeTypeHole"
                }
            }
        ]
    }
}